<div class="container">
  <h1>Cocktails</h1>
  <div class="cards">
    <% @cocktails.each do |cocktail| %>
      <div class="card-category">
        <%= link_to cocktail.name, cocktail_path(cocktail) %>
      </div>
    <% end %>
  </div> 

  <%= link_to "Create a Cocktail", new_cocktail_path %>
  

</div>
